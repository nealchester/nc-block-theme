{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank Template"
		}
	],
	"settings": {
		"appearanceTools": true,
		"blocks": {
			"core/image": {
				"lightbox": {
					"allowEditing": false,
					"enabled": false
				}
			}
		},
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#222222",
					"name": "Charcoal",
					"slug": "charcoal"
				},
				{
					"color": "#666666",
					"name": "Gray Dark",
					"slug": "gray-dark"
				},
				{
					"color": "#aaaaaa",
					"name": "Gray Medium",
					"slug": "gray-medium"
				},
				{
					"color": "#cccccc",
					"name": "Gray Light",
					"slug": "gray-light"
				},
				{
					"color": "#f5f5f5",
					"name": "Smoke",
					"slug": "smoke"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				}
			],
			"text": true
		},
		"layout": {
			"contentSize": "var(--width-standard)",
			"wideSize": "var(--width-wide)"
		},
		"shadow": {
			"defaultPresets": true,
			"presets": [
				{
					"name": "Theme",
					"shadow": "var(--shadows)",
					"slug": "theme"
				}
			]
		},
		"spacing": {
			"customSpacingSize": true,
			"spacingScale": {
					"increment": 1.5,
					"mediumStep": 1.5,
					"operator": "*",
					"steps": 7,
					"unit": "rem"
			},
			"spacingSizes": [
				{
					"name": "Space 1/3",
					"size": "0.3rem",
					"slug": "20"
				},
				{
					"name": "Space 1/2",
					"size": "0.5rem",
					"slug": "30"
				},
				{
					"name": "Space 1",
					"size": "1rem",
					"slug": "40"
				},
				{
					"name": "Space 1.5",
					"size": "1.5rem",
					"slug": "50"
				},
				{
					"name": "Space 2",
					"size": "2rem",
					"slug": "60"
				},
				{
					"name": "Space 2.5",
					"size": "2.5rem",
					"slug": "70"
				},
				{
					"name": "Space 3",
					"size": "3rem",
					"slug": "80"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"vmin",
				"vmax",
				"%"
			]
		},
		"typography": {
			"customFontSize": true,
			"defaultFontSizes": false,
			"dropCap": true,
			"fontFamilies": [
				{
					"fontFamily": "var(--sans)",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"fontFamily": "var(--serif)",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"fontFamily": "var(--mono)",
					"name": "Mono",
					"slug": "mono"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "0.75rem",
						"min": "0.625rem"
					},
					"name": "Tiny",
					"size": "0.75rem",
					"slug": "tiny"
				},
				{
					"fluid": {
						"max": "0.875rem",
						"min": "0.75rem"
					},
					"name": "Small / h6",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.2rem",
						"min": "0.95rem"
					},
					"name": "Normal / h5",
					"size": "1rem",
					"slug": "normal"
				},
				{
					"fluid": {
						"max": "1.5rem",
						"min": "1.125rem"
					},
					"name": "Medium / h4",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.875rem",
						"min": "1.5rem"
					},
					"name": "Large / h3",
					"size": "1.875rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2.5rem",
						"min": "1.875rem"
					},
					"name": "X-Large / h2",
					"size": "2.5rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2.5rem"
					},
					"name": "XX-Large / h1",
					"size": "3rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"max": "3.75rem",
						"min": "3rem"
					},
					"name": "Huge",
					"size": "3.75rem",
					"slug": "huge"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textColumns": true,
			"textDecoration": false,
			"textTransform": true,
			"writingMode": false
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--charcoal)"
				},
				"spacing": {
					"padding": {
						"bottom": "0.5rem",
						"left": "1.5em",
						"right": "1.5em",
						"top": "0.5rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"border": {
					"bottom": {
						"color": "var(--wp--preset--color--charcoal)",
						"style": "solid",
						"width": "2px"
					},
					"left": {
						"color": "var(--wp--preset--color--charcoal)",
						"style": "solid",
						"width": "2px"
					},
					"right": {
						"color": "var(--wp--preset--color--charcoal)",
						"style": "solid",
						"width": "2px"
					},
					"top": {
						"color": "var(--wp--preset--color--charcoal)",
						"style": "solid",
						"width": "2px"
					}
				},
				"variations": {
					"outline": {
						"spacing": {
							"padding": {
								"bottom": "0.5rem",
								"left": "1.5em",
								"right": "1.5em",
								"top": "0.5rem"
							}
						},
						"typography": {
							"fontStyle": "normal",
							"fontWeight": "500"
						},
						"border": {
							"bottom": {
								"color": "currentColor",
								"style": "solid",
								"width": "2px"
							},
							"left": {
								"color": "currentColor",
								"style": "solid",
								"width": "2px"
							},
							"right": {
								"color": "currentColor",
								"style": "solid",
								"width": "2px"
							},
							"top": {
								"color": "currentColor",
								"style": "solid",
								"width": "2px"
							}
						}
					}
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"bottom": "1rem"
					}
				}
			},
			"core/list": {
				"spacing": {
					"margin": {
						"bottom": "1rem"
					}
				}
			},
			"core/navigation": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--30)"
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": {
						"bottom": "1rem"
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontStyle": "italic",
					"fontWeight": "400"
				}
			},
			"core/post-featured-image": {
				"spacing": {
					"margin": {
						"bottom": "1.5rem",
						"top": "1.5rem"
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "1",
					"textTransform": "uppercase"
				}
			},
			"core/table": {
				"spacing": {
					"margin": {
						"bottom": "1rem"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "0rem",
			"margin": {
				"bottom": "0px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			},
			"padding": {
				"bottom": "0px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--sans)",
			"fontSize": "var(--txt-base)",
			"lineHeight": "1.5"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "post-listing",
			"title": "Post Listing"
		}
	],
	"version": 3
}